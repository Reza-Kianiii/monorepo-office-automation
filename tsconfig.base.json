{
  "compilerOptions": {
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "customConditions": ["development"],
    "baseUrl": ".",
    "paths": {
      "@office-automation/dashboard-shell/feature/home": [
        "libs/dashboard-shell/feature/home/src/index.ts"
      ],
      "@office-automation/dashboard/data/menu": [
        "libs/dashboard/data/menu/src/index.ts"
      ],
      "@office-automation/dashboard/feature/shell": [
        "libs/dashboard/feature/shell/src/index.ts"
      ],
      "@office-automation/shared/data/permission": [
        "libs/shared/data/permission/src/index.ts"
      ],
      "@office-automation/shared/ui/drawer": [
        "libs/shared/ui/drawer/src/index.ts"
      ],
      "@office-automation/shared/util/mui-theme": [
        "libs/shared/util/mui-theme/src/index.ts"
      ],
      "@office-automation/workflow-engine/data/workflow-engine-menu": [
        "libs/workflow-engine/data/workflow-engine-menu/src/index.ts"
      ],
      "@office-automation/workflow-engine/feature/new-case": [
        "libs/workflow-engine/feature/new-case/src/index.ts"
      ],
      "@office-automation/workflow-engine/feature/workflow-engine-shell": [
        "libs/workflow-engine/feature/workflow-engine-shell/src/index.ts"
      ],
      "@office-automation/workflow-engine/utils/redux-store": [
        "libs/workflow-engine/utils/redux-store/src/index.ts"
      ]
    }
  }
}
